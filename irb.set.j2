{% for i in switch_vars %}
set interfaces xe-0/0/12 vlan-tagging
set interfaces xe-0/0/12 mtu 9192
set interfaces xe-0/0/12 unit {{ i.xe-0/0/12_prefix }}{{ i.vlan_id }} description "MFD325-MDF-DSW1 int xe-0/0/0 {{ i.role }}"
set interfaces xe-0/0/12 unit {{ i.xe-0/0/12_prefix }}{{ i.vlan_id }} vlan-id {{ i.xe-0/0/12_prefix }}{{ i.vlan_id }}
set interfaces xe-0/0/12 unit {{ i.xe-0/0/12_prefix }}{{ i.vlan_id }} family inet address 169.254.{{ i.vlan_id }}.{{ i.xe-0/0/12_octet }}/30
set interfaces irb unit {{ i.irb_prefix }}{{ i.vlan_id }} description "MFD325-MDF-FW1 int ethernet1/3 {{ i.role }}"
set interfaces irb unit {{ i.irb_prefix }}{{ i.vlan_id }} family inet address 169.254.{{ i.vlan_id }}.{{ i.irb_net_oct }}/30
set vlans trust_{{ i.role }}_palo_p2p vlan-id {{ i.irb_prefix }}{{ i.vlan_id }}
set vlans trust_{{ i.role }}_palo_p2p l3-interface irb.{{ i.irb_prefix }}{{ i.vlan_id }}
{% endfor %}
